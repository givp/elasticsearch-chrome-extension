{
    "manifest_version": 2,
    "name": "Elasticsearch Console",
    "description": "Developer console for ElasticSearch.",
    "version": "0.0.1",
    "icons": {
        "16": "icons/logo16.png",
        "48": "icons/logo48.png",
        "128": "icons/logo128.png"
    },
    "permissions": [
        "tabs",
        "clipboardWrite",
        "http://*/*",
        "https://*/*",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ],
    "browser_action": {
        "default_icon": {
            "19": "icons/logo19.png",
            "38": "icons/logo38.png"
        }
    },
    "background": {
        "scripts": [
            "src/background.js"
        ]
    }
}
